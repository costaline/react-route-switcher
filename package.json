{
  "version": "0.1.0",
  "license": "MIT",
  "name": "@costaline/react-route-switcher",
  "author": "Constantine Zubarev <kv.zubarev@gmail.com>",
  "description": "Component to simplify route configuration",
  "keywords": [
    "react",
    "router"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/costaline/react-route-switcher.git"
  },
  "bugs": {
    "url": "https://github.com/costaline/react-route-switche/issues"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-route-switcher.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/react-route-switcher.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-route-switcher.esm.js",
      "limit": "10 KB"
    }
  ],
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "peerDependencies": {
    "react": ">=16",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.5",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.0",
    "dts-cli": "^0.17.1",
    "husky": "^7.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^5.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  }
}
